<header>
		<nav>
            <a href="#"><img src="../images/arrowLeft.svg"></a>
            <a href="#"><img src="../images/edit.svg"></a>
        </nav>
        <ul>
            <li>
                <h3>{{account/dogsCount}}</h2>
                <h4>Honden</h3>
            </li>
            <li>
                <h1>{{account/firstName}}</h1>
                <h2>{{account/lastName}}</h2>
            </li>
            <li>
                <a href="#" class="display"><img src="../images/star.svg"></a>
                <a href="#"><img src="../images/starFilled.svg"></a>
            </li>
        </ul>
        <img src="{{account.profileImg}}">
	</header>

	
	
	<main>
        <ul>
            <li>
                <img src="../images/location.svg">
                <h4>{{account/city}}</h4>
            </li>
            <li>
                <h3>{{account/age}}</h3>
                <h4>Jaar</h4>
            </li>
        </ul>

        <h3>Over mij</h3>
        <p>
            {{account.about}}
        </p>

        <h3>Honden</h3>
        <ul>

            {{#each dogs}}
                <a href="#">
                    <li>
                        <img src="{{this.imageUrl}}">
                        <h5>{{this.dogName}}</h5>
                    </li>
                </a>
            {{/each}}

        </ul>

        <span><a href="#">Chat met {{account/firstName}}!</a></span>
	</main>

	<script src="../js/script.js"></script>