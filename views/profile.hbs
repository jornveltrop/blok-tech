<body class="profile">
    <header>
        <nav>
            <a href="../"><img src="/images/arrowLeft.svg"></a>
            <a href="#"><img src="/images/edit.svg"></a>
        </nav>
        <ul>
            <li>
                <h3>{{profiel/dogsCount}}</h2>
                <h4>Honden</h3>
            </li>
            <li>
                <h1>{{profiel/firstName}}</h1>
                <h2>{{profiel/lastName}}</h2>
            </li>
            <li>
                <a href="#" class="display"><img src="/images/star.svg"></a>
                <a href="#"><img src="/images/starFilled.svg"></a>
            </li>
        </ul>
        <img src="{{profiel.profileImg}}">
    </header>



    <main>
        <ul>
            <li>
                <img src="/images/location.svg">
                <h4>{{profiel/city}}</h4>
            </li>
            <li>
                <h3>{{profiel/age}}</h3>
                <h4>Jaar</h4>
            </li>
        </ul>

        <h3>Over mij</h3>
        <p>
            {{profiel.about}}
        </p>

        <h3>Honden</h3>
        <ul>

            {{#each dogs}}
                <a href="#">
                    <li>
                        <img src="{{this.imageUrl}}">
                        <h5>{{this.dogName}}</h5>
                    </li>
                </a>
            {{/each}}

        </ul>

        <span><a href="#">Chat met {{profiel/firstName}}!</a></span>
    </main>

    <script src="/js/script.js"></script>
</body>